<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PointServlet</servlet-name>
    <servlet-class>cn.hc.Servlet.PointServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>monthSearchServlet</servlet-name>
    <servlet-class>cn.hc.Servlet.monthSearchServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>pieSearchServlet</servlet-name>
    <servlet-class>cn.hc.Servlet.pieSearchServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>monthCountServlet</servlet-name>
    <servlet-class>cn.hc.Servlet.monthCountServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>PointServlet</servlet-name>
    <url-pattern>/PointServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>monthSearchServlet</servlet-name>
    <url-pattern>/monthSearchServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>pieSearchServlet</servlet-name>
    <url-pattern>/pieSearchServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>monthCountServlet</servlet-name>
    <url-pattern>/monthCountServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
